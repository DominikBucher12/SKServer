use_frameworks!
  
target 'SKServer macOS' do
        platform :osx, '10.14'
        pod 'SKCore'
        pod 'SKWebAPI'
        pod 'Swifter', :git => 'https://github.com/httpswift/swifter.git', :branch => 'stable'
end

target 'SKServer iOS' do
        platform :ios, '12.0'
        pod 'SKCore'
        pod 'SKWebAPI'
        pod 'Swifter', :git => 'https://github.com/httpswift/swifter.git', :branch => 'stable'
end

target 'SKServer tvOS' do
        platform :tvos, '12.0'
        pod 'SKCore'
        pod 'SKWebAPI'
        pod 'Swifter', :git => 'https://github.com/httpswift/swifter.git', :branch => 'stable'
end
